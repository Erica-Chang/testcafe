{
  "name": "testcafe",
  "version": "1.0.0",
  "description": "Automation Testing for Chegg",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "testcafe": "^1.8.5"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:chrome": "testcafe chrome ./tests -s takeOnFails=true",
    "test:chrome:5": "testcafe -c 5 chrome ./tests -s takeOnFails=true",
    "test:chrome:headless": "testcafe chrome:headless ./tests -s takeOnFails=true",
    "test:chrome:heeadless:5": "testcafe -c 5 chrome:headless ./tests -s takeOnFails=true",
    "test:chrome:moblie": "testcafe chrome:emulation:device=iphone 11 ./tests -s takeOnFails=true"
  },
  "author": "",
  "license": "ISC"
}
